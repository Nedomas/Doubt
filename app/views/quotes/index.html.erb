<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Open</th>
    <th>High</th>
    <th>Low</th>
    <th>Close</th>
    <th>Volume</th>
    <th>Adjclose</th>
    <th>Stock</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.date %></td>
    <td><%= quote.open %></td>
    <td><%= quote.high %></td>
    <td><%= quote.low %></td>
    <td><%= quote.close %></td>
    <td><%= quote.volume %></td>
    <td><%= quote.adjClose %></td>
    <td><%= quote.stock_id %></td>
    <td><%= link_to 'Show', quote %></td>
    <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
    <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote', new_quote_path %>
